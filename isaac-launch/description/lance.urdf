<?xml version="1.0"?>
<robot name="lance" xmlns:xacro="http://www.ros.org/wiki/xacro">
<!-- ^^ xacro is needed to resolve the mesh filepaths -->

	<link name = "base_link">
		<visual>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-body-tags.dae" />
			</geometry>
		</visual>
	</link>

<!-- FRAME -->
	<joint name = "frame_joint" type = "fixed">
		<parent link = "base_link"/>
		<child link = "frame_link"/>
		<origin xyz = "0 0 0.13103567"/>
	</joint>
	<link name = 'frame_link'>
		<inertial>
			<origin xyz = "0.101 0.001 0.214"/>
			<mass value="9.773"/>
			<inertia
				ixx = "1.358"
				ixy = "-0.001"
				ixz = "-0.418"
				iyy = "1.677"
				iyz = "0.000"
				izz = "1.579"
			/>
		</inertial>

		<visual name = 'frame_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-frame.dae" />
			</geometry>
		</visual>
		<collision name = 'frame_collision'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-frame-collision.stl" />
			</geometry>
		</collision>

		<visual name = 'frame_push_plate_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-frame-push-plate.dae" />
			</geometry>
		</visual>
		<collision name = 'frame_push_plate_collision'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-frame-push-plate.dae" />
			</geometry>
		</collision>
	</link>


<!-- LEFT TRACK -->
	<joint name = 'left_track_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "left_track_link" />
		<origin xyz = "0.20318559 0.28988800 0.00242022"/>
	</joint>
	<link name = 'left_track_link'>
		<inertial>
			<origin xyz = "-0.162 0.015 0.021"/>
			<mass value="7.888"/>
			<inertia
				ixx = "0.103"
				ixy = "-0.014"
				ixz = "-0.049"
				iyy = "0.403"
				iyz = "0.014"
				izz = "0.331"
			/>
		</inertial>

		<visual name = 'left_track_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-left-track.dae" />
			</geometry>
		</visual>

		<collision name = 'left_track_collision'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-left-track-collision.stl" />
			</geometry>
		</collision>
	</link>

<!-- RIGHT TRACK -->
	<joint name = 'right_track_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "right_track_link" />
		<origin xyz = "0.20318559 -0.28988800 0.00242022"/>
	</joint>
	<link name = 'right_track_link'>
		<inertial>
			<origin xyz = "-0.162 -0.015 0.021"/>
			<mass value="7.888"/>
			<inertia
				ixx = "0.103"
				ixy = "-0.014"
				ixz = "-0.049"
				iyy = "0.403"
				iyz = "-0.014"
				izz = "0.331"
			/>
		</inertial>

		<visual name = 'right_track_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-right-track.dae" />
			</geometry>
		</visual>

		<collision name = 'right_track_collision'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-right-track-collision.stl" />
			</geometry>
		</collision>
	</link>


<!-- Excavation and Dump Unit -->
	<joint name = 'dump_joint' type = 'revolute'>
		<parent link = "frame_link" />
		<child link = "collection_link" />

		<origin
			xyz = "-0.45721441 0 0.18580822"
			rpy = "0 -0.2617994 0"
		/>

		<axis xyz = "0 1 0" />
		<limit
			upper = "0.2617994"
			lower = "-0.2617994"
			effort = "100"
			velocity = "100"
		/>
		<dynamics damping = "0.0" friction = "0.0" />
	</joint>
	<link name = 'collection_link'>
		<inertial>
			<origin xyz = "0.417 -0.02 0.038"/>
			<mass value="20.499"/>
			<inertia
				ixx = "1.005"
				ixy = "-0.122"
				ixz = "-0.283"
				iyy = "2.472"
				iyz = "0.027"
				izz = "2.445"
			/>
		</inertial>

		<visual name = 'hopper_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-hopper.dae" />
			</geometry>
		</visual>
		<visual name = 'collection_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-collection.dae" />
			</geometry>
		</visual>

		<collision name = 'collection_collision'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-upper-collision.stl" />
			</geometry>
		</collision>
	</link>


<!-- LIDAR -->
	<joint name = 'lidar_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "lidar_link" />
		<origin
			xyz = "0.64957717 0 0.42478867"
			rpy = "2.836255 -0.171851 -2.067879"
		/>
	</joint>
	<link name = 'lidar_link'>
		<inertial>
			<origin xyz = "1.97e-5 -0.001 -0.009"/>
			<mass value="0.7"/>
			<inertia
				ixx = "0.001"
				ixy = "2e-8"
				ixz = "4e-7"
				iyy = "0.001"
				iyz = "-3e-5"
				izz = "0.001"
			/>
		</inertial>

		<visual name = 'lidar_visual'>
			<geometry>
				<mesh filename = "file://$(find isaac_launch)/description/lance/meshes/lance-2024-multiscan.dae" />
			</geometry>
		</visual>

		<collision name = 'lidar_collision'>
			<origin xzy = "0 0 -0.006" />
			<geometry>
				<cylinder
					radius = "0.048"
					length = "0.098"
				/>
			</geometry>
		</collision>
	</link>
	<joint name = 'lidar_joint_inv_rotated' type = 'fixed'>
		<parent link = "lidar_link" />
		<child link = "lidar_link_inv_rotated" />
		<origin
			xyz = "0 0 0"
			rpy = "0 0 2.09439510239"
		/>
	</joint>
	<link name = 'lidar_link_inv_rotated'></link>

<!-- Forward Camera -->
	<joint name = 'fwd_cam_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "fwd_cam_link" />
		<origin
			xyz = "0.72468246 0 0.51148737"
			rpy = "-1.74532925199 0 -1.57079632679"
		/>
	</joint>
	<link name = 'fwd_cam_link'></link>

<!-- Right Camera -->
	<joint name = 'rght_cam_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "rght_cam_link" />
		<origin
			xyz = "-0.06416965 -0.28448800 0.22203322"
			rpy = "-1.57079632679 0 3.1415926"
		/>
	</joint>
	<link name = 'rght_cam_link'></link>

<!-- Left Camera -->
	<joint name = 'left_cam_joint' type = 'fixed'>
		<parent link = "frame_link" />
		<child link = "left_cam_link" />
		<origin
			xyz = "-0.06416965 0.28448800 0.22203322"
			rpy = "-1.57079632679 0 0"
		/>
	</joint>
	<link name = 'left_cam_link'></link>

</robot>
